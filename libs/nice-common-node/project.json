{
  "name": "nice-common-node",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/nice-common-node/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/nice-common-node",
        "main": "libs/nice-common-node/src/index.ts",
        "tsConfig": "libs/nice-common-node/tsconfig.lib.json",
        "assets": [
          "libs/nice-common-node/*.md"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/nice-common-node/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/nice-common-node/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
